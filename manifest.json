{


    "manifest_version": 3,

    "name": "Proxy Connector Made By Muhammad Haris",
    "description": "This Extension made By Muhammad Haris For Any Type Proxy Connect",
    "version": "1.0",

    "default_locale": "en",

    "permissions": [
        "storage",
        "proxy",
        "webRequest",
        "webRequestAuthProvider"
    ],

    "host_permissions": [
        "<all_urls>"
    ],

    "action": {
        "default_popup": "popup/dist/index.html"
    },

    "background": {
        "service_worker": "background.js"
    },

    "icons": {
        "16": "icons/logo.png",
        "32": "icons/logo.png",
        "48": "icons/logo.png",
        "128": "icons/logo.png"
    }
}
